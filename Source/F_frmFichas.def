    Exit Sub
Error_btnEntradaViajeros_Click:
    Select Case Err
        Case Else
            MsgBox "error nº " & Err & " en btnEntradaViajeros_Click" & vbCrLf & Err.Description
            Resume Salir_btnEntradaViajeros_Click
    End Select
